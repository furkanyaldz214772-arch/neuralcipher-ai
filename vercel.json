{
  "buildCommand": "cd frontend && rm -rf .next && rm -rf node_modules/.cache && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://neuralcipher-ai-production.up.railway.app",
    "CACHE_BUST": "2026-01-28-v2"
  }
}
